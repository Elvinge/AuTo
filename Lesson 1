import subprocess

def check_command_output(command, text_to_check):
    try:
        # Выполняем команду и получаем её вывод
        output = subprocess.check_output(command, shell=True, text=True)
        
        # Проверяем, содержится ли искомый текст в выводе
        if text_to_check in output:
            return True
        else:
            return False
    except subprocess.CalledProcessError:
        # Если команда завершилась ошибкой, возвращаем False
        return False

# Пример использования:
command_to_execute = "echo 'Lara, Sara, Kara'"  # Ваша команда
text_to_find = "Lara, Sara, Kara"  # Текст для поиска в выводе команды

result = check_command_output(command_to_execute, text_to_find)

if result:
    print("Команда выполнена успешно и текст найден в выводе.")
else:
    print("Команда не выполнена успешно или текст не найден в выводе.")
